#include<iostream>
using namespace std;
int main()
{
	int a, b;
	cout << "Please input two integers: ";
	cin >> a >> b;
	if (a > b)
	{
		int temp = a;
		a = b;
		b = temp;
	}
	cout << "The number from " << a << " to " << b << " is: ";
	while (a <= b)
	{
		cout << a << ' ';
		++a;
	}
	cout << endl;
	system("pause");
	return 0;
}
